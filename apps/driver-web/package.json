{
  "name": "@mishwari/driver-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run copy-fonts && next dev -p 3001",
    "build": "npm run copy-fonts && next build",
    "start": "next start -p 3001",
    "lint": "next lint",
    "copy-fonts": "node -e \"const fs=require('fs');const path=require('path');const src=path.join(__dirname,'../../packages/ui-web/public/fonts');const dest=path.join(__dirname,'public/fonts');if(!fs.existsSync(dest))fs.mkdirSync(dest,{recursive:true});fs.readdirSync(src).forEach(f=>fs.copyFileSync(path.join(src,f),path.join(dest,f)));\""
  },
  "dependencies": {
    "@mishwari/api": "workspace:*",
    "@mishwari/types": "workspace:*",
    "@mishwari/features-auth": "workspace:*",
    "@mishwari/ui-web": "workspace:*",
    "@mishwari/ui-primitives": "workspace:*",
    "@fortawesome/fontawesome-svg-core": "^6.4.2",
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.1.3",
    "@nextui-org/react": "^2.1.13",
    "@reduxjs/toolkit": "^1.9.7",
    "axios": "^1.6.1",
    "date-fns": "^2.30.0",
    "framer-motion": "^11.5.6",
    "jwt-decode": "^4.0.0",
    "next": "15.1.3",
    "next-redux-wrapper": "^8.1.0",
    "react": "^18",
    "react-day-picker": "^8.8.2",
    "react-dom": "^18",
    "react-redux": "^8.1.3",
    "react-toastify": "^9.1.3",
    "redux": "^4.2.1",
    "redux-persist": "^6.0.0",
    "redux-persist-transform-filter": "^0.0.22",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10",
    "postcss": "^8",
    "tailwindcss": "^3.4.14",
    "typescript": "^5"
  }
}
